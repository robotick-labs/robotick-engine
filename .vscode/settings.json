{
    "cmake.useCMakePresets": "always",
    "cmake.configurePreset": "ubuntu-x64-debug",
    "cmake.buildPreset": "ubuntu-x64-debug",
    "cmake.sourceDirectory": "${workspaceFolder}",
    "cmake.buildDirectory": "${workspaceFolder}/build",
    "cmake.testExplorer.enableOutput": true,
    "cmake.testExplorer.useCTest": true,
    "cmake.testExplorer.extraCTestArgs": "--test-dir ${workspaceFolder}/build/ubuntu-x64-debug/cpp/tests",
    "testMate.cpp.testFramework": "catch2",
    "testMate.cpp.enableTestDiscovery": true,
    "testMate.cpp.debug.config.template": "Debug Robotick Tests (No Leak Check)",
    "C_Cpp.default.compileCommands": "${workspaceFolder}/build/ubuntu-x64-debug/compile_commands.json",
    "C_Cpp.codeAnalysis.clangTidy.enabled": true,
    "C_Cpp.codeAnalysis.clangTidy.checks.disabled": [
        "modernize-use-trailing-return-type"
    ],
    "terminal.integrated.defaultProfile.linux": "bash",
    "editor.formatOnSave": true,
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "clang-format.style": "file",
    "search.exclude": {
        "**/lib": true,
        "**/components/robotick-engine": true
    },
    "files.exclude": {
        "**/components/robotick-engine": true
    },
    "files.watcherExclude": {
        "**/components/robotick-engine/**": true
    },
    "search.useGlobalIgnoreFiles": true,
    "files.associations": {
        "type_traits": "cpp",
        "any": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "bitset": "cpp",
        "cctype": "cpp",
        "charconv": "cpp",
